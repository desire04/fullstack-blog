{"ast":null,"code":"import{useState,useEffect}from'react';import{getAuth,onAuthStateChanged}from'firebase/auth';const useUser=()=>{const[user,setUser]=useState(null);const[isLoading,setIsLoading]=useState(true);useEffect(()=>{const unsubscribe=onAuthStateChanged(getAuth(),user=>{setUser(user);setIsLoading(false);});return unsubscribe;},[]);return{user,isLoading};};export default useUser;","map":{"version":3,"names":["useState","useEffect","getAuth","onAuthStateChanged","useUser","user","setUser","isLoading","setIsLoading","unsubscribe"],"sources":["/Users/zahyoo/FullstackBlog/my-blog/src/hooks/useUser.js"],"sourcesContent":["import { useState, useEffect } from 'react';\nimport { getAuth, onAuthStateChanged } from 'firebase/auth';\n\nconst useUser = () => {\n    const [user, setUser] = useState(null);\n    const[isLoading, setIsLoading] = useState(true);\n\n    useEffect(() => {\n        const unsubscribe = onAuthStateChanged(getAuth(), user => {\n            setUser(user);\n            setIsLoading(false);\n        });\n\n        return unsubscribe;\n    }, []);\n\n    return { user, isLoading };\n}\n\nexport default useUser;"],"mappings":"AAAA,OAASA,QAAQ,CAAEC,SAAS,KAAQ,OAAO,CAC3C,OAASC,OAAO,CAAEC,kBAAkB,KAAQ,eAAe,CAE3D,KAAM,CAAAC,OAAO,CAAGA,CAAA,GAAM,CAClB,KAAM,CAACC,IAAI,CAAEC,OAAO,CAAC,CAAGN,QAAQ,CAAC,IAAI,CAAC,CACtC,KAAK,CAACO,SAAS,CAAEC,YAAY,CAAC,CAAGR,QAAQ,CAAC,IAAI,CAAC,CAE/CC,SAAS,CAAC,IAAM,CACZ,KAAM,CAAAQ,WAAW,CAAGN,kBAAkB,CAACD,OAAO,CAAC,CAAC,CAAEG,IAAI,EAAI,CACtDC,OAAO,CAACD,IAAI,CAAC,CACbG,YAAY,CAAC,KAAK,CAAC,CACvB,CAAC,CAAC,CAEF,MAAO,CAAAC,WAAW,CACtB,CAAC,CAAE,EAAE,CAAC,CAEN,MAAO,CAAEJ,IAAI,CAAEE,SAAU,CAAC,CAC9B,CAAC,CAED,cAAe,CAAAH,OAAO","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}